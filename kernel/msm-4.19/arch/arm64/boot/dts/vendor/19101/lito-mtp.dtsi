&soc {
	gpio_keys {
		compatible = "gpio-keys";
		label = "gpio-keys";

		pinctrl-names = "default";
		//#ifdef VENDOR_EDIT
		//Fei.Mo@BSP.group 2018-09-01 Modity for vol key
		pinctrl-0 = <&key_vol_up_default &key_vol_down_default>;
		//#endif

		vol_up {
			label = "volume_up";
			gpios = <&pm8150l_gpios 5 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEUP>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
		//#ifdef VENDOR_EDIT
		//Fei.Mo@BSP.group 2018-09-01 Add for vol key
		vol_down {
			label = "volume_down";
			gpios = <&pm7250b_gpios  3 GPIO_ACTIVE_LOW>;
			linux,input-type = <1>;
			linux,code = <KEY_VOLUMEDOWN>;
			gpio-key,wakeup;
			debounce-interval = <15>;
			linux,can-disable;
		};
		//#endif
	};

	vdda_usb_ss_dp_core: vdda_usb_ss_dp_core {
		compatible = "regulator-fixed";
		regulator-name = "vdd_supply";
		regulator-min-microvolt = <880000>;
		regulator-max-microvolt = <880000>;
/*#ifdef VENDOR_EDIT */
/* yufeng@camera.driver. 2019/08/19, remove gpio for camera */
		//enable-active-high;
		//gpio = <&pm8150l_gpios 12 GPIO_ACTIVE_HIGH>;
		//pinctrl-names = "default";
		//pinctrl-0 = <&usb_eldo13>;
/*endif */
	};

	mtp_batterydata: qcom,battery-data {
//#ifndef VENDOR_EDIT
//Yichun.Chen@BSP.CHG.Basic  2018/05/10  Delete for use oppo battery
//		qcom,batt-id-range-pct = <15>;
//#else
        qcom,batt-id-range-pct = <19>;
//#endif
		#include "qg-batterydata-alium-3600mah.dtsi"
		#include "qg-batterydata-atl466271_3300mAh.dtsi"
	};
	// #ifdef VENDOR_EDIT
	/* Ping.Liu@Bsp.Fingerprint.Secure 2019-10-16 add for secure proc */
	oppo_secure_common {
		compatible = "oppo,secure_common";
	};
	//#endif //VENDOR_EDIT
	//#ifdef VENDOR_EDIT /*VENDOR_EDIT*/
	/*Bangxiong.Wu@BSP.Fingerprint.Basic,2019-04-16 add for fingerprint*/
	oppo_fp_common {
		compatible = "oppo,fp_common";
		oppo,fp_gpio = <65>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_id2_default>;
		goodix_optical {
			oppo,fp-id = <0>; /* fp id mapping table */
			vendor-chip = <11>; /* please refer to the struct fp_vendor_t in oppo_fp_commmon.n */
			eng-menu = "-1,-1"; /* represents SNR, inclination test item in engineer menu, and 0/1 means off/on */
			chip-name = "G_OPTICAL_G5";  /* /proc/fp_id, will be displayed in engineer menu  */
		};
	};

	/*Bangxiong.Wu@BSP.Fingerprint.Basic,2019-04-16 add for goodix fingerprint*/
	goodix_fp {
		status = "ok";
		compatible = "goodix,goodix_fp";
		interrupt-parent = <&tlmm>;
		interrupts = <112 0>;
		power-mode = <1> ; /*please refer to the struct fp_power_mode_t in oppo_fp_commmon.n ,0: power mode not set, 1: ldo power, 2: gpio power, 3: auto power */
		ldo-supply = <&pm8150a_l9>; // pm660l_l5 for goodix_fp
		ldo-config = <3300000 3300000 150000>;
		gpio-power = <&pm8150_gpios 7 GPIO_ACTIVE_LOW>;
		goodix,gpio_irq = <&tlmm 112 0x1>;
		goodix,gpio_reset = <&tlmm 63 0x0>;
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_goodix_irq_default>;
		notify_tpinfo_flag = <1>;
		power-num = <1> ;
		power_source_1 {
			power-mode = <1>;   /* 1 = ldo power, refer to oppo_fp_common.h*/
			power-name = "ldo";
			power-config = "ldo-config";
			delay-time = <0>;
		};
	};
	//#endif /*VENDOR_EDIT*/

	/*xing.xiong@BSP.Kernel.Driver, 2019/09/10, Add for adc read for aboard*/
	devinfo {
		compatible = "oppo-devinfo";

		io-channels= <&pm7250b_vadc ADC_GPIO1>;
		io-channel-names= "vph_pwr_voltage_sub";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio11_adc_default>;

		main-sub {
			compatible = "hw-match, main-sub";
			use_pmic_adc;
			adc_ranges = <0 80
						81 180
						181 300
						301 420
						421 550
						551 650
						651 760
						761 850
						851 850
						951 1050
						1051 1150
						1151 1300>;
			aboard-patterns = <0 0 1>;

			/*main board*/
			match-projects = <19101 19102 19501>;
		};
	};
	//#ifdef VENDOR_EDIT
	/*Murphy@BSP.sensor, 2019/09/10, Add for sensor dts*/
	oppo_sensor {
		status = "ok";
		compatible = "sensor-devinfo";
		ps-match-id = <1>; //stk33502 bit[0]
		als-match-id = <1>; //stk33502 bit[0]
		als-type = <3>; //normal
		ps-type = <1>; //normal
		ps-cali-type = <1>; //SOFTWARE_CAIL
		is-unit-device = <1>; //two in one device
		is-als-dri = <1>; //polling
		irq-number = <111>;
		bus-number = <5>;
		als-factor = <151>;
		ak-msensor-dir = <0>;
		mx-msensor-dir = <0>;
		st-gsensor-dir = <7>;
		bs-gsensor-dir = <0>;
		als-row-coe = <151>;
	};
	//#endif /*VENDOR_EDIT*/
	//#ifdef VENDOR_EDIT
	/* Fuchun.Liao@BSP.CHG.Basic 2018/11/26 modify for oppo rf cable monitor */
	oppo_rf_cable {
		compatible = "oppo,rf_cable";
		interrupt-parent = <&tlmm>;
		rf_cable_0_support;
		// rf_cable_1_support;
		rf,cable0-gpio = <&tlmm 90 0>;
		// rf,cable1-gpio = <&tlmm 58 0>;
		rf,cable-support-timer = <0>;
		pinctrl-names = "rf_cable_active";
		pinctrl-0 = <&rf_cable0_active>;
	};
	//#endif /* VENDOR_EDIT */

	//#ifdef VENDOR_EDIT
	/*Murphy@BSP.sensor, 2019/09/10, Add for NC gpio*/
	gpio_pvt {
		status = "ok";
		compatible = "oppo_gpio_pvt";
		pinctrl-names = "default";
		pinctrl-0 = <&gpio_pvt_nc &gpio_pvt_7250B_nc &gpio_pvt_7150A_nc &gpio_pvt_nc_out_low>;
	};
	//#endif
};

